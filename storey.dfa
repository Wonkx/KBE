	  Operation; Subtract;
	  Target; {base:};
  };
  
  (Child) ap4: {
	  class; ug_block;
	  Length; (storeyLength:/2)-storeyWallThickness:*2;
	  Width; (storeyWidth:)/2-storeyWallThickness:-1-storeyWallThickness:;
	  Height; storeyHeight:-storeyWallThickness:;
	  Origin; storeyOrigin: + vector(storeyLength:/2+0.3, 11.3, storeyWallThickness:);
	  Operation; Subtract;
	  Target; {base:};
  };
 
 
